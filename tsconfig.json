{
    "compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"isolatedModules": true,
        "module": "esnext",
        "target": "es5",
	"lib": ["ES2017"],
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
		"noEmitOnError": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
        "pretty": true,
        "noEmit": true
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}
